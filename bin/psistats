#!/usr/bin/env python
import sys
import os

if __name__ == "__main__":
    try:
        from psistats import cli
    except ImportError:
        sys.path.append(os.path.realpath(os.path.dirname(__file__) + "/../"))
        from psistats import cli
    cli.main(sys.argv)
else:
    raise RuntimeError("This file should not be imported")
